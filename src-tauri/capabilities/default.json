{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "http:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "https://naughty-bookmarks.vercel.app/**" },
        { "url": "https://*.supabase.co/**" },
        { "url": "http://localhost:3000/**" }
      ],
      "deny": []
    }
  ]
}
